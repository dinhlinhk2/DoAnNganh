@model Demo.ModelsView.MuaHangSuccessVM

<div class="about-banner different-bg-position section-space-y-axis-100" data-bg-image="assets/images/about/banner/1-1-1920x598.jpg" style="background-image: url(&quot;assets/images/about/banner/1-1-1920x598.jpg&quot;);">
	<div class="container">
		<div class="row">
			<div class="col-xl-6 col-lg-5">
				<div class="parallax-img-wrap">
